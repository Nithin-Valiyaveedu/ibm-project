          <h1><%= name%></h1>
      <div class="wrap-table100">
        <div class="table100">
          <table>
            <thead>
              <!--Values to be fetched from database-->
              <tr class="table100-head">
                <th class="column1">Product</th>
                <th class="column2">25</th>
            </thead>
            <tbody>
              <!--Values to be fetched from database-->
              <% data.forEach(function(value){%>
                <tr>
                  <td class="column1"><%= value.item %></td>
                  <td class="column2"><%= value.count %></td>
                </tr>
              <%}); %> 
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
        <div>
    <form action=<%= "/user/placeorder/"+ id%> method="POST">
        <input type="text" name="item" placeholder="Enter Item name">
        <input type="text" name="count" placeholder="Enter Quantity">
        <input type="radio" id="volunteer" name="option" value="volunteer">
        <label for="male">Volunteer Needed</label><br>
        <input type="radio" id="self" name="option" value="selfservice">
        <label for="female">Self Service</label><br>
        <input type="submit" name="submit" id="submit" class="form-submit" value="Place Order"/>
    </form>
  </div>
      <footer class="page-footer font-small footer_bg">
        <div class="footer-copyright text-center  py-3"><b> Team-Hacksterz</b>
        </div>
    </footer>
  


